{
  "name": "sports",
  "version": "0.0.1",
  "description": "",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "license": "Apache-2.0",
  "keywords": [],
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./build/src/mikro-orm.config.js"
    ]
  },
  "scripts": {
    "start:ts-node": "ts-node src/index.ts",
    "prestart": "yarn run compile",
    "start": "node build/src/index.js",
    "test": "jest",
    "check": "gts check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "clean": "gts clean",
    "precompile": "yarn run clean && yarn run codegen && yarn run fix",
    "compile": "tsc -p .",
    "postcompile": "copyfiles \"src/**/*.graphql\" build",
    "fix": "gts fix \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepare": "yarn run compile",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/introspection": "1.12.2",
    "@graphql-codegen/typescript": "1.12.2",
    "@graphql-codegen/typescript-resolvers": "1.12.2",
    "@types/jest": "^25.1.3",
    "@types/node": "^10.17.16",
    "apollo-server-testing": "^2.10.1",
    "copyfiles": "^2.2.0",
    "gts": "^2.0.0-alpha.4",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "~3.8.2"
  },
  "dependencies": {
    "@graphql-toolkit/file-loading": "^0.9.7",
    "apollo-datasource": "^0.7.0",
    "apollo-server": "^2.10.1",
    "dataloader": "^2.0.0",
    "graphql": "^14.6.0",
    "graphql-scalars": "^1.0.6",
    "graphql-tools": "^4.0.7",
    "graphql-tools-fork": "^8.6.1",
    "mikro-orm": "^3.3.1",
    "pg": "^7.18.2"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
